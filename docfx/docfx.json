{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "maxParallelism": 1,
    "content": [
      {
	    "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/*.yml"
        ]
      }
    ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}